{
  "name": "oh-my-props",
  "version": "1.0.0",
  "description": "Validate & transform your object's properties by schema.",
  "main": "main.js",
  "dependencies": {
    "uiterator": "^1.0.1"
  },
  "devDependencies": {
    "mocha": "^8",
    "xo": "latest"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirick13/oh-my-props.git"
  },
  "keywords": [
    "props",
    "properties",
    "validate",
    "validator",
    "transform",
    "args",
    "arguments"
  ],
  "author": "Daniil Kirichenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kirick13/oh-my-props/issues"
  },
  "homepage": "https://github.com/kirick13/oh-my-props#readme",
  "xo": {
    "root": true,
    "envs": [
      "es2020"
    ],
    "rules": {
      "array-bracket-spacing": [
        "error",
        "always",
        {
          "arraysInArrays": false,
          "objectsInArrays": false
        }
      ],
      "arrow-parens": "off",
      "brace-style": [
        "warn",
        "stroustrup"
      ],
      "camelcase": "off",
      "capitalized-comments": "off",
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "complexity": [
        "warn",
        100
      ],
      "computed-property-spacing": "off",
      "func-names": "off",
      "import/no-absolute-path": "off",
      "indent": "off",
      "key-spacing": "off",
      "keyword-spacing": [
        "warn"
      ],
      "max-depth": [
        "warn",
        100
      ],
      "new-cap": [
        "error",
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "no-await-in-loop": "off",
      "no-constant-condition": "off",
      "no-else-return": "off",
      "no-lonely-if": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-multi-assign": "off",
      "no-multi-spaces": "off",
      "no-negated-condition": "off",
      "no-new": "off",
      "no-lone-blocks": "off",
      "no-promise-executor-return": "off",
      "no-throw-literal": "off",
      "no-unused-expressions": [
        "error",
        {
          "allowShortCircuit": true
        }
      ],
      "no-unused-vars": "warn",
      "object-curly-spacing": "off",
      "padding-line-between-statements": "off",
      "prefer-const": "warn",
      "prefer-promise-reject-errors": "off",
      "quote-props": "off",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "radix": [
        "error",
        "as-needed"
      ],
      "space-before-blocks": [
        "warn"
      ],
      "space-before-function-paren": [
        "warn",
        "always"
      ],
      "space-infix-ops": "off",
      "spaced-comment": [
        "warn"
      ],
      "strict": [
        "off",
        "global"
      ],
      "yoda": "off",
      "no-use-extend-native/no-use-extend-native": "off",
      "unicorn/filename-case": "off",
      "unicorn/no-lonely-if": "off",
      "unicorn/prefer-ternary": "off",
      "unicorn/prefer-type-error": "off",
      "unicorn/prevent-abbreviations": "off"
    }
  }
}
